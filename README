│   ├── backup_coremail.sh
│   ├── backup_mysql.sh
│   ├── boot_coremail.sh
│   ├── count_orgcos.sh
│   ├── cre_del_cmsa.sh
│   ├── kill_coremail.sh
│   └── remove_coremail.sh
├── conf
│   ├── CentOS-Base.repo
│   ├── header
│   ├── hosts
│   ├── rsyncd.conf
│   └── vimrc
├── cron
│   ├── ftpsync.sh
│   ├── killsvnftp.sh
│   ├── svnsync.sh
│   └── upctools.sh
├── init.sh
├── README
